package modelo;

public class Reserva {
	// Atributos
	private int id;
	private String origem;
	private String destino;
	private int ida;
	private int volta;
	private double valor;
	private int acompanhantes;
	
	//Construtores
	public Reserva () {
		
	}
	public Reserva(int id, String origem, String destino, int ida, int volta, double valor, int acompanhantes) {
		this.id = id;
		this.origem = origem;
		this.destino = destino;
		this.ida = ida;
		this.volta = volta;
		this.valor = valor;
		this.acompanhantes = acompanhantes;
	}
	
	//Gets e Sets
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getOrigem() {
		return origem;
	}

	public void setOrigem(String origem) {
		this.origem = origem;
	}

	public String getDestino() {
		return destino;
	}

	public void setDestino(String destino) {
		this.destino = destino;
	}

	public int getIda() {
		return ida;
	}

	public void setIda(int ida) {
		this.ida = ida;
	}

	public int getVolta() {
		return volta;
	}

	public void setVolta(int volta) {
		this.volta = volta;
	}

	public double getValor() {
		return valor;
	}

	public void setValor(double valor) {
		this.valor = valor;
	}

	public int getAcompanhantes() {
		return acompanhantes;
	}

	public void setAcompanhantes(int acompanhantes) {
		this.acompanhantes = acompanhantes;
	}
	
	//MÃ©todos
	@Override
	public String toString() {
		return "Reserva [id=" + id + ", origem=" + origem + ", destino=" + destino + ", ida=" + ida + ", volta=" + volta
				+ ", valor=" + valor + ", acompanhantes=" + acompanhantes + "]";
	}

}
